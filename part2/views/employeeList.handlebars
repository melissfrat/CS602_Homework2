<b>Employees with Last Name {{lastName}}</b>
<table border=1>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    {{#each employees}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
    </tr>
  {{/each}}
</table>'


